{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "pcl-proto",
  "version": "0.5.2",
  "identifier": "pcl-community.pcl.proto",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173/",
    "beforeDevCommand": "bun dev",
    "beforeBuildCommand": "bun run build"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}